{
  "extends": ["eslint:recommended", "prettier"],
  "globals": {
    "firebase": true,
    "chrome": true,
    "userData": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "curly": "warn",
    "no-case-declarations": "off"
  }
}
