{
    "name": "GitHub issues submitter",
    "version": "1.0",
    "description": "Submits issues directly to github from the browser window",
    "key":"",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    }, 
    "permissions": [
        "identity"
    ]
}